using System;
using System.Collections.Generic;
using System.Text;

{{>namespace info.title}} {

  public static class Info 
  {
    {{#each _info}}
    public static {{#if (isObj this) }}{{capitalizeFirst @key}}{{else}}String{{/if}} {{capitalizeFirst @key}} { get; set; } =  
    {{#if (isObj this) }}
        new {{capitalizeFirst @key}} {
    {{#each this}}
            {{capitalizeFirst @key}}= "{{this}}",
    {{/each}}
        };

    {{else}}
        "{{this}}";
    {{/if}}
    {{/each}}
  }

  {{#each _info}}
  {{#if (isObj this) }}
  public class {{capitalizeFirst @key}}
  {
      {{#each this}}
      public String {{capitalizeFirst @key}} {get; set;}
      {{/each}}
  }
  
  {{/if}}
  {{/each}}
}
